<dx:ThemedWindow
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
    xmlns:local="clr-namespace:ThiTracNghiem"
    xmlns:dxsps="http://schemas.devexpress.com/winfx/2008/xaml/spreadsheet"
    xmlns:dxrud="http://schemas.devexpress.com/winfx/2008/xaml/reports/userdesigner"
    x:Class="ThiTracNghiem.DXWindow1"
    Title="DXWindow1" Height="478" Width="1014"
    Loaded="ThemedWindow_Loaded"
    >
    <dx:ThemedWindow.Resources>
        <local:VanPhongDataSet x:Key="vanPhongDataSet"/>
        <CollectionViewSource x:Key="databaseViewSource" Source="{Binding Database, Source={StaticResource vanPhongDataSet}}"/>
    </dx:ThemedWindow.Resources>

    <Grid x:Name="grid1" Margin="0,0,224,-1"  >
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="180*"/>
            <ColumnDefinition Width="211*"/>
        </Grid.ColumnDefinitions>
        <ScrollViewer Margin="0,10,0,0" Grid.ColumnSpan="2" HorizontalAlignment="Left" Width="782">
            <StackPanel x:Name="stackpanel">

            </StackPanel>
        </ScrollViewer>

    </Grid>

</dx:ThemedWindow>
